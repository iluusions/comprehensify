{
  "manifest_version": 3,
  "name": "comprehensify",
  "description": "App that personalizes text according to the reader's comprehension level",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "identity"
  ],
  "host_permissions": [
     "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "oauth2": {
    "client_id": "46137983121-901uhunn5eq87841f68ummortte87loi.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
